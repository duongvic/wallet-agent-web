<script>
  (function (document, window, $) {
    'use strict';
    var Site = window.Site;
    $(document).ready(function () {
      Site.run();
    });
  })(document, window, jQuery);

  var ctx = "${pageContext.request.contextPath}";
  $(document).ready(function () {
    var token = "${_csrf.token}";
    var header = "${_csrf.parameterName}";
    $.ajaxSetup({
      beforeSend: function (xhr) {
        if (header && token) {
          xhr.setRequestHeader(header, token);
        }
      }
    });

    /*Change language*/
    var searchUrl = location.search;
    if (searchUrl.indexOf('?') > -1) {
      if (searchUrl.indexOf('lang=en') > -1) {
        $("#langEn").attr("href", searchUrl);
        $("#langVi").attr("href", searchUrl.replace('lang=en', "lang=vi"));
      } else if (searchUrl.indexOf('lang=vi') > -1) {
        $("#langEn").attr("href", searchUrl.replace('lang=vi', "lang=en"));
        $("#langVi").attr("href", searchUrl);
      } else {
        $("#langEn").attr("href", searchUrl + '&lang=en');
        $("#langVi").attr("href", searchUrl + '&lang=vi');
      }
    } else {
      $("#langEn").attr("href", searchUrl + '?lang=en');
      $("#langVi").attr("href", searchUrl + '?lang=vi');
    }
  });

  /*Chỉ nhập số*/
  function validateNumber(elem) {
    var validChars = /[0-9]/;
    var strIn = elem.value;
    var strOut = '';
    for (var i = 0; i < strIn.length; i++) {
      strOut += (validChars.test(strIn.charAt(i))) ? strIn.charAt(i) : '';
    }
    elem.value = strOut;
  }

  /*  999-9999-9999*/
  function validateSTK() {
    var x = document.getElementById("validationSTK").value;
    var y = x.replace(/[-]+/g, "");
    var z = y.replace(/[A-Za-z0-9]{4}(?!$)/g, "\$&" + "-");
    document.getElementById("validationSTK").value = z;
  }

  function isNumberKey(evt) {
    var charCode = (evt.which) ? evt.which : event.keyCode
    if (charCode > 31 && (charCode < 48 || charCode > 57))
      return false;
    else {
      return true;
    }
  }

  /*Show pass*/
  function mouseoverPass(obj) {
    var obj = document.getElementById('showPassword');
    obj.type = "text";
  }

  function mouseoutPass(obj) {
    var obj = document.getElementById('showPassword');
    obj.type = "password";
  }

  function mouseoverPassV2(str) {
    var obj = document.getElementById(str);
    obj.type = "text";
  }

  function mouseoutPassV2(str) {
    var obj = document.getElementById(str);
    obj.type = "password";
  }

  /*  fix select2 title*/
  $(document).ready(function () {
    $('.select2-selection__rendered').hover(function () {
      $(this).removeAttr('title');
    });

    $("[role=log].ui-helper-hidden-accessible").remove();

    // Up-case and remove special character
    var ctkTag = $(".ctk");
    if (ctkTag !== null && ctkTag !== undefined) {
      if (ctkTag.html() !== undefined) {
        console.log(utfToNormal(ctkTag.html()));
        ctkTag.html(utfToNormal(ctkTag.html()));
      }

      if (ctkTag.val() !== undefined) {
        console.log(utfToNormal(ctkTag.val()));
        ctkTag.val(utfToNormal(ctkTag.val()));
      }
    }
  });
</script>

